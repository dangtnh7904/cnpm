# ========================================
# DATABASE CONFIGURATION
# ========================================
# Thông tin kết nối đến SQL Server
DB_HOST=localhost
DB_PORT=1433
DB_NAME=QuanLyNhanKhauDB
DB_USERNAME=your_username_here
DB_PASSWORD=your_password_here

# ========================================
# JPA CONFIGURATION
# ========================================
# Hiển thị SQL queries trong console (true/false)
JPA_SHOW_SQL=false

# ========================================
# SERVER CONFIGURATION
# ========================================
# Port của Spring Boot server
SERVER_PORT=8080

# ========================================
# LOGGING CONFIGURATION
# ========================================
# Mức độ log (TRACE, DEBUG, INFO, WARN, ERROR)
LOG_LEVEL=INFO
SQL_LOG_LEVEL=INFO
